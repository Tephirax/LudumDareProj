/// scr_assigngrowth();
switch ( planted ) {
    case -1: // Unplanted
        growthdaytarget = -1;
        witherdaytarget = -1;
        break;
    case 0: // Onions
        growthdaytarget = 2;
        witherdaytarget = 4;
        break;
    case 1: // Tomatoes
        growthdaytarget = 3;
        witherdaytarget = 3;
        break;
    case 2: // Lettuce
        growthdaytarget = 4;
        witherdaytarget = 2;
        break;
    case 3: // Flower 1
        growthdaytarget = 4;
        witherdaytarget = 4;
        break;
    case 4: // Flower 2
        growthdaytarget = 4;
        witherdaytarget = 4;
        break;
    case 5: // Flower 3
        growthdaytarget = 4;
        witherdaytarget = 4;
        break;
    
}
        

